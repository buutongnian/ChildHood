<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
	<class name="edu.buu.childhood.my.pojo.UserRegInf" table="T_SYS_USER_REG">
		<id name="userName" column="USER_NAME">
			<generator class="assigned" ></generator>
		</id>		
		<property name="regDate" column="REG_DATE" update="false"/>
		<property name="regLatitude" column="REG_LATITUDE" update="false"/>
		<property name="regLongitude" column="REG_LONGITUDE" update="false"/>
		<property name="belongingArea" column="REG_LATITUDE" update="false"/>
		<property name="liveCommunity" column="REG_LONGITUDE" update="false"/>
	</class>
</hibernate-mapping>

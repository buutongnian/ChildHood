<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
	<class name="edu.buu.childhood.common.GameAge" table="T_PUB_GAME_AGE">
		<id name="ageCode" column="AGE_CODE">
			<generator class="native" ></generator>
		</id>		
		<property name="ageInterval" column="AGE_INTERVAL" update="false"/>
	</class>
</hibernate-mapping>

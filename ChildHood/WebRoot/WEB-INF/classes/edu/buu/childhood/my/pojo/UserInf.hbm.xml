<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
	<class name="edu.buu.childhood.my.pojo.UserInf" table="T_SYS_USER_INF">
		<id name="userName" column="USER_NAME">
			<generator class="assigned" ></generator>
		</id>
		<!-- 注意：可更新字段更新前一定要先取出愿数据，存放在JavaBean中后再对相应JavaBean要修改字段进行赋值操作！ -->		
		<property name="userPwd" column="USER_PWD"/>
		<property name="userHeadImage" column="USER_HEADIMAGE"/>
		<property name="userNickname" column="USER_NICKNAME"/>
		<property name="lastLatitude" column="LAST_LATITUDE"/>
		<property name="lastLongitude" column="LAST_LONGITUDE"/>
		<property name="belongingProvince" column="BELONGING_PROVINCE"/>
		<property name="belongingCity" column="BELONGING_CITY"/>
		<property name="belongingDistrict" column="BELONGING_DISTRICT"/>
		<property name="liveCommunity" column="LIVE_COMMUNITY"/>
		<property name="detailAddr" column="DETAIL_ADDR"/>
		<property name="loginStatus" column="LOGIN_STATUS"/>
		<many-to-one name="userRegInf" class="edu.buu.childhood.my.pojo.UserRegInf" column="USER_NAME" unique="true" not-null="true" insert="false" update="false"></many-to-one>
		<many-to-one name="parentInf" class="edu.buu.childhood.my.pojo.ParentInf" column="PARENT_INF" unique="true" not-null="true"></many-to-one>
		<set name="childInfSet" table="T_SYS_CHILD_INF">
			<key>
				<column name="USER_NAME" not-null="true" />
			</key>
			<one-to-many class="edu.buu.childhood.my.pojo.ChildInf"/>
		</set>
	</class>
</hibernate-mapping>

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
	<class name="edu.buu.childhood.status.pojo.UserCanJoin" table="V_STATUS_USER_CANJOIN">
	<composite-id>
		<key-property name="userName" column="USER_NAME"></key-property>
		<key-property name="lastLatitude" column="LAST_LATITUDE"></key-property>
		<key-property name="lastLongitude" column="LAST_LONGITUDE"></key-property>
	</composite-id>
	</class>
</hibernate-mapping>
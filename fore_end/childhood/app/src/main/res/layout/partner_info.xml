<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#eaeff3">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="200dp">

        <edu.buu.childhood.my.pojo.RoundImageView
            android:id="@+id/roundImageView"
            android:layout_centerInParent="true"
            android:layout_width="96dp"
            android:layout_height="96dp"
            android:src="@drawable/touxiang"
            />
        <TextView
            android:id="@+id/partnerInfoNi"
            android:layout_below="@+id/roundImageView"
            android:layout_centerInParent="true"
            android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="36px"
            android:textColor="#000000"
            android:text="lcccc"
            />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginTop="9dp"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="12dp"
            android:background="@color/my_bar_color">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:orientation="horizontal">
                <TextView
                    android:layout_marginLeft="20dp"
                    style="@style/getPartner_text"
                    android:text="地理位置:"
                    />
                <TextView
                    android:id="@+id/partnerInfoProvince"
                    android:layout_marginLeft="10dp"
                    style="@style/getPartner_text"
                    android:text="123"
                    />
                <TextView
                    android:layout_marginLeft="10dp"
                    style="@style/getPartner_text"
                    android:text="省"
                    />
                <TextView
                    android:id="@+id/partnerInfoCity"
                    android:layout_marginLeft="10dp"
                    style="@style/getPartner_text"
                    android:text="123"
                    />
                <TextView
                    android:layout_marginLeft="10dp"
                    style="@style/getPartner_text"
                    android:text="市"
                    />
                <TextView
                    android:id="@+id/partnerInfoDistrict"
                    android:layout_marginLeft="10dp"
                    style="@style/getPartner_text"
                    android:text="123"
                    />
                <TextView
                    android:layout_marginLeft="10dp"
                    style="@style/getPartner_text"
                    android:text="区"
                    />
            </LinearLayout>
            <View style="@style/PersonalLine" />
            <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:orientation="horizontal">
            <TextView
                android:layout_marginLeft="20dp"
                style="@style/getPartner_text"
                android:text="详细地址:"
                />
            <TextView
                android:id="@+id/partnerInfoDetailAddr"
                android:layout_marginLeft="10dp"
                style="@style/getPartner_text"
                android:text="123"
                />
        </LinearLayout>
            <View style="@style/PersonalLine" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:orientation="horizontal">
                <TextView
                    android:layout_marginLeft="20dp"
                    style="@style/getPartner_text"
                    android:text="邮箱账号:"
                    />
                <TextView
                    android:id="@+id/partnerInfoEmail"
                    android:layout_marginLeft="10dp"
                    style="@style/getPartner_text"
                    android:text="123"
                    />
            </LinearLayout>
            <View style="@style/PersonalLine" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:orientation="horizontal">
                <TextView
                    android:layout_marginLeft="20dp"
                    style="@style/getPartner_text"
                    android:text="他（她）的等级:"
                    />
                <TextView
                    android:id="@+id/partnerInfoRank"
                    android:layout_marginLeft="10dp"
                    style="@style/getPartner_text"
                    android:text="123"
                    />
            </LinearLayout>
            <View style="@style/PersonalLine" />
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:orientation="horizontal">
                <TextView
                    android:layout_marginLeft="20dp"
                    style="@style/getPartner_text"
                    android:text="他（她）的成就:"
                    />
                <TextView
                    android:id="@+id/partnerInfoAchievement"
                    android:layout_marginLeft="10dp"
                    style="@style/getPartner_text"
                    android:text="123"
                    />

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginTop="9dp"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="12dp"
            android:background="@color/my_bar_color">
            <LinearLayout
                android:id="@+id/parentInfo"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:orientation="horizontal">
                <TextView
                    android:layout_marginLeft="20dp"
                    style="@style/getPartner_text"
                    android:text="家长信息"
                    />
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginTop="9dp"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="12dp"
            android:background="@color/my_bar_color">
            <LinearLayout
                android:id="@+id/childInfo"
                android:layout_width="match_parent"
                android:layout_height="48dp"
                android:orientation="horizontal">
                <TextView
                    android:layout_marginLeft="20dp"
                    style="@style/getPartner_text"
                    android:text="孩子信息"
                    />

            </LinearLayout>


        </LinearLayout>

    </LinearLayout>

</LinearLayout>
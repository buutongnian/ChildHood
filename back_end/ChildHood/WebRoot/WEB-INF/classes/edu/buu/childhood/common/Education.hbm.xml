<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
	<class name="edu.buu.childhood.common.Education" table="T_PUB_EDUCATION">
		<id name="educationCode" column="EDUCATION_CODE">
			<generator class="native" ></generator>
		</id>		
		<property name="educationName" column="EDUCATION_NAME" update="false"/>
	</class>
</hibernate-mapping>

<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
	<class name="edu.buu.childhood.achvmt.medal.pojo.UserMedalList" table="V_ACHVMT_USER_MEDAL">
	<composite-id>
		<key-property name="userName" column="USER_NAME"></key-property>
		<key-property name="medalId" column="MEDAL_ID"></key-property>
		<key-property name="medalDescribe" column="MEDAL_DESCRIBE"></key-property>
		<key-property name="getDate" column="GET_DATE"></key-property>
		<key-property name="medalName" column="MEDAL_NAME"></key-property>
	</composite-id>
	</class>
</hibernate-mapping>

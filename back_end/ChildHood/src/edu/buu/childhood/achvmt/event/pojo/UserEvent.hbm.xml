<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>
	<class name="edu.buu.childhood.achvmt.event.pojo.UserEvent" table="T_ACHVMT_USER_EVENT_LIST">
		<id name="listId" column="LIST_ID">
			<generator class="identity" ></generator>
		</id>
		<property name="userName" column="USER_NAME"/>
		<property name="eventDate" column="EVENT_DATE"/>
		<property name="eventTemplate" column="EVENT_TEMPLATE"/>
		<property name="eventTemplateName" column="EVENT_TEMPLATE_NAME"/>
		<property name="eventParams" column="EVENT_PARAMS"/>
	</class>
</hibernate-mapping>
